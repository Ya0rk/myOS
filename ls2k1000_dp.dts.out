/dts-v1/;

/ {
	model = "LS2K1000-DP-V10";
	compatible = "loongson,ls2k";
	#address-cells = <0x02>;
	#size-cells = <0x02>;

	aliases {
		ethernet0 = "/2k1000-soc/ethernet@40040000";
		ethernet1 = "/2k1000-soc/ethernet@40050000";
		serial0 = "/2k1000-soc/serial@0x1fe20000";
		serial1 = "/2k1000-soc/serial@0x1fe20100";
		serial2 = "/2k1000-soc/serial@0x1fe20200";
		serial3 = "/2k1000-soc/serial@0x1fe20300";
		serial4 = "/2k1000-soc/serial@0x1fe20400";
		serial5 = "/2k1000-soc/serial@0x1fe20500";
		serial6 = "/2k1000-soc/serial@0x1fe20600";
		serial7 = "/2k1000-soc/serial@0x1fe20700";
		serial8 = "/2k1000-soc/serial@0x1fe20800";
		serial9 = "/2k1000-soc/serial@0x1fe20900";
		serial10 = "/2k1000-soc/serial@0x1fe20a00";
		serial11 = "/2k1000-soc/serial@0x1fe20b00";
		i2c0 = "/2k1000-soc/i2c@1fe21000";
		i2c1 = "/2k1000-soc/i2c@1fe21800";
	};

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs = "earlycon";
	};

	cpus {
		#address-cells = <0x01>;
		#size-cells = <0x00>;

		cpu@0 {
			device_type = "cpu";
			compatible = "loongarch";
			reg = <0x00>;
			numa-node-id = <0x00>;
		};

		cpu@1 {
			device_type = "cpu";
			compatible = "loongarch";
			reg = <0x01>;
			numa-node-id = <0x00>;
		};
	};

	interrupt-controller {
		compatible = "loongson,cpu-interrupt-controller";
		interrupt-controller;
		#interrupt-cells = <0x01>;
		phandle = <0x01>;
	};

	interrupt-controller@1fe01400 {
		compatible = "loongson,2k1000-icu";
		interrupt-controller;
		#interrupt-cells = <0x01>;
		reg = <0x00 0x1fe01400 0x00 0x40 0x00 0x1fe01040 0x00 0x10>;
		interrupt-parent = <0x01>;
		interrupt-names = "cascade";
		interrupts = <0x03>;
		phandle = <0x06>;
	};

	pinirq_ctrl@1fe00530 {
		compatible = "pinctrl-single";
		reg = <0x00 0x1fe00530 0x00 0x08>;
		#address-cells = <0x01>;
		#size-cells = <0x00>;
		#pinctrl-cells = <0x02>;
		pinctrl-single,bit-per-mux;
		pinctrl-single,register-width = <0x20>;
		pinctrl-single,function-mask = <0x01>;
		status = "disabled";
	};

	2k1000-soc {
		compatible = "ls,nbus\0simple-bus";
		#address-cells = <0x02>;
		#size-cells = <0x02>;
		ranges = <0x00 0x10000000 0x00 0x10000000 0x00 0x10000000 0x00 0x2000000 0x00 0x2000000 0x00 0x2000000 0x00 0x20000000 0x00 0x20000000 0x00 0x10000000 0x00 0x40000000 0x00 0x40000000 0x00 0x40000000 0xfe 0x00 0xfe 0x00 0x00 0x40000000>;
		dma-coherent;

		osc-clock {
			#clock-cells = <0x00>;
			compatible = "fixed-clock";
			clock-frequency = <0x5f5e100>;
			clock-output-names = "ref_clk";
			phandle = <0x02>;
		};

		clock-controller@1fe00480 {
			compatible = "loongson,ls2x-clk";
			reg = <0x00 0x1fe00480 0x00 0x01>;
			clocks = <0x02>;
			clock-names = "ref_clk";
			#clock-cells = <0x01>;
			phandle = <0x05>;
		};

		pinctrl@1fe00420 {
			compatible = "loongson,2k1000-pinctrl";
			reg = <0x00 0x1fe00420 0x00 0x18>;

			gmac1 {
				phandle = <0x14>;

				mux {
					groups = "gmac1";
					function = "gmac1";
				};
			};

			hda {
				phandle = <0x1b>;

				mux {
					groups = "hda_i2s";
					function = "hda";
				};
			};

			hda_as_gpio {

				mux {
					groups = "hda_i2s";
					function = "gpio";
				};
			};

			i2s {
				phandle = <0x1c>;

				mux {
					groups = "hda_i2s";
					function = "i2s";
				};
			};

			ahci {
				phandle = <0x0e>;

				mux {
					groups = "sata_led";
					function = "sata_led";
				};
			};

			ahci_led_as_gpio {

				mux {
					groups = "sata_led";
					function = "gpio";
				};
			};

			pwm0 {
				phandle = <0x0f>;

				mux {
					groups = "pwm0";
					function = "pwm0";
				};
			};

			pwm0_as_gpio {

				mux {
					groups = "pwm0";
					function = "gpio";
				};
			};

			pwm1 {
				phandle = <0x10>;

				mux {
					groups = "pwm1";
					function = "pwm1";
				};
			};

			pwm1_as_gpio {

				mux {
					groups = "pwm1";
					function = "gpio";
				};
			};

			pwm2 {
				phandle = <0x11>;

				mux {
					groups = "pwm2";
					function = "pwm2";
				};
			};

			pwm2_as_gpio {

				mux {
					groups = "pwm2";
					function = "gpio";
				};
			};

			pwm3 {
				phandle = <0x12>;

				mux {
					groups = "pwm3";
					function = "pwm3";
				};
			};

			pwm3_as_gpio {

				mux {
					groups = "pwm3";
					function = "gpio";
				};
			};

			uart0_4 {
				phandle = <0x04>;

				mux {
					groups = "uart0";
					function = "uart0_4";
				};
			};

			uart1_4 {
				phandle = <0x07>;

				mux {
					groups = "uart1";
					function = "uart1_4";
				};
			};

			uart2_4 {
				phandle = <0x08>;

				mux {
					groups = "uart2";
					function = "uart2_4";
				};
			};

			dvo0 {

				mux {
					groups = "dvo0_lio_uart";
					function = "dvo0";
				};
			};

			dvo1 {
				phandle = <0x0c>;

				mux {
					groups = "dvo1_camera";
					function = "dvo1";
				};
			};

			can0 {
				phandle = <0x19>;

				mux {
					groups = "can0";
					function = "can0";
				};
			};

			can1 {
				phandle = <0x1a>;

				mux {
					groups = "can1";
					function = "can1";
				};
			};

			can1_as_gpio {

				mux {
					groups = "can1";
					function = "gpio";
				};
			};

			sdio {

				mux {
					groups = "sdio";
					function = "gpio";
				};
			};

			lio {
				phandle = <0x03>;

				mux {
					groups = "dvo0_lio_uart";
					function = "lio";
				};
			};

			i2c0_as_gpio {

				mux {
					groups = "i2c0";
					function = "gpio";
				};
			};

			i2c1_as_gpio {

				mux {
					groups = "i2c1";
					function = "gpio";
				};
			};
		};

		localio@0x1c100000 {
			compatible = "loongson,lio";
			reg = <0x00 0x1c100000 0x00 0x3b00000 0x00 0x1fd00000 0x00 0x100000 0x00 0x1ff00000 0x00 0x100000>;
			lio-big-mem = <0x00>;
			lio-io-width = <0x01>;
			lio-io-count-init = <0x1f>;
			lio-rom-width = <0x01>;
			lio-rom-count-init = <0x1f>;
			lio-clk-period = <0x00>;
			pinctrl-0 = <0x03>;
			pinctrl-names = "default";
			status = "disabled";
		};

		serial@0x1fe20000 {
			compatible = "ns16550a";
			pinctrl-names = "default";
			pinctrl-0 = <0x04>;
			reg = <0x00 0x1fe20000 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x00>;
			no-loopback-test;
			status = "okay";
		};

		serial@0x1fe20100 {
			compatible = "ns16550a";
			pinctrl-names = "default";
			pinctrl-0 = <0x07>;
			reg = <0x00 0x1fe20100 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x00>;
			no-loopback-test;
			status = "okay";
		};

		serial@0x1fe20200 {
			compatible = "ns16550a";
			pinctrl-names = "default";
			pinctrl-0 = <0x08>;
			reg = <0x00 0x1fe20200 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x00>;
			no-loopback-test;
			status = "okay";
		};

		serial@0x1fe20300 {
			compatible = "ns16550a";
			reg = <0x00 0x1fe20300 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x00>;
			no-loopback-test;
			status = "okay";
		};

		serial@0x1fe20400 {
			compatible = "ns16550a";
			reg = <0x00 0x1fe20400 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x01>;
			no-loopback-test;
			status = "okay";
		};

		serial@0x1fe20500 {
			compatible = "ns16550a";
			reg = <0x00 0x1fe20500 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x01>;
			no-loopback-test;
			status = "okay";
		};

		serial@0x1fe20600 {
			compatible = "ns16550a";
			reg = <0x00 0x1fe20600 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x01>;
			no-loopback-test;
			status = "okay";
		};

		serial@0x1fe20700 {
			compatible = "ns16550a";
			reg = <0x00 0x1fe20700 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x01>;
			no-loopback-test;
			status = "disable";
		};

		serial@0x1fe20800 {
			compatible = "ns16550a";
			reg = <0x00 0x1fe20800 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x02>;
			no-loopback-test;
			status = "disable";
		};

		serial@0x1fe20900 {
			compatible = "ns16550a";
			reg = <0x00 0x1fe20900 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x02>;
			no-loopback-test;
			status = "disable";
		};

		serial@0x1fe20a00 {
			compatible = "ns16550a";
			reg = <0x00 0x1fe20a00 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x02>;
			no-loopback-test;
			status = "disable";
		};

		serial@0x1fe20b00 {
			compatible = "ns16550a";
			reg = <0x00 0x1fe20b00 0x00 0x10>;
			clocks = <0x05 0x0c>;
			interrupt-parent = <0x06>;
			interrupts = <0x02>;
			no-loopback-test;
			status = "disable";
		};

		gpio@0x1fe00500 {
			compatible = "loongson,loongson3-gpio";
			reg = <0x00 0x1fe00500 0x00 0x38>;
			ngpios = <0x40>;
			gpio_base = <0x00>;
			conf_offset = <0x00>;
			out_offset = <0x10>;
			in_offset = <0x20>;
			int_offset = <0x30>;
			in_start_bit = <0x00>;
			gpio-controller;
			#gpio-cells = <0x02>;
			support_irq;
			interrupt-parent = <0x06>;
			interrupts = <0x3c 0x3d 0x3e 0x3f 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3a 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b 0x3b>;
			phandle = <0x0a>;
		};

		syscon@0x1fe27000 {
			compatible = "syscon";
			reg = <0x00 0x1fe27000 0x00 0x58>;
			phandle = <0x09>;
		};

		reboot {
			compatible = "syscon-reboot";
			regmap = <0x09>;
			offset = <0x30>;
			mask = <0x01>;
		};

		poweroff {
			compatible = "syscon-poweroff";
			regmap = <0x09>;
			offset = <0x14>;
			mask = <0x3c00>;
			value = <0x3c00>;
		};

		otg@40000000 {
			compatible = "loongson,loongson2-dwc2";
			reg = <0x00 0x40000000 0x00 0x40000>;
			interrupt-parent = <0x06>;
			interrupts = <0x31>;
			dma-mask = <0x00 0xffffffff>;
			status = "okay";
			loongson,dwc2-fix = <0x00>;
			dr_mode = "host";
		};

		ohci@40070000 {
			compatible = "loongson,ls2k-ohci\0generic-ohci";
			reg = <0x00 0x40070000 0x00 0x8000>;
			interrupt-parent = <0x06>;
			interrupts = <0x33>;
			dma-mask = <0x00 0xffffffff>;
		};

		ehci@40060000 {
			compatible = "loongson,ls2k-ehci\0generic-ehci";
			reg = <0x00 0x40060000 0x00 0x8000>;
			interrupt-parent = <0x06>;
			interrupts = <0x32>;
			dma-mask = <0x00 0xffffffff>;
		};

		i2c@1fe21000 {
			compatible = "loongson,ls-i2c";
			reg = <0x00 0x1fe21000 0x00 0x08>;
			interrupt-parent = <0x06>;
			interrupts = <0x16>;
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			clocks = <0x05 0x0c>;
			clock-names = "i2c-clk";
			status = "okay";

			eeprom@57 {
				compatible = "atmel,24c32";
				reg = <0x57>;
				pagesize = <0x20>;
			};
		};

		i2c@1fe21800 {
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			compatible = "loongson,ls-i2c";
			reg = <0x00 0x1fe21800 0x00 0x08>;
			interrupt-parent = <0x06>;
			interrupts = <0x17>;
			clocks = <0x05 0x0c>;
			clock-names = "i2c-clk";
			status = "okay";

			gt911@5d {
				compatible = "goodix,gt911";
				reg = <0x5d>;
				interrupt-parent = <0x06>;
				interrupts = <0x3c>;
				irq-gpios = <0x0a 0x00 0x01>;
			};

			gt9147@14 {
				compatible = "goodix,gt9147";
				reg = <0x14>;
				interrupt-parent = <0x06>;
				interrupts = <0x3c>;
				irq-gpios = <0x0a 0x00 0x01>;
			};
		};

		dc@400c0000 {
			compatible = "loongson,ls2k1000-dc";
			reg = <0x00 0x400c0000 0x00 0x10000>;
			interrupt-parent = <0x06>;
			interrupts = <0x1c>;
			dma-mask = <0x00 0xffffffff>;
			lsdc,relax_alignment;
			status = "okay";
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			memory-region = <0x0b>;

			dvo@1 {
				compatible = "lsdc,dvo";
				reg = <0x01>;
				pinctrl-0 = <0x0c>;
				pinctrl-names = "default";
			};

			ports {
				#address-cells = <0x01>;
				#size-cells = <0x00>;

				port@1 {
					reg = <0x01>;

					endpoint {
						remote-endpoint = <0x0d>;
						phandle = <0x23>;
					};
				};
			};
		};

		gpu@40080000 {
			compatible = "vivante,gc";
			reg = <0x00 0x40080000 0x00 0x40000>;
			interrupt-parent = <0x06>;
			interrupts = <0x1d>;
			dma-mask = <0x00 0xffffffff>;
		};

		ahci@400e0000 {
			compatible = "snps,spear-ahci";
			reg = <0x00 0x400e0000 0x00 0x10000>;
			interrupt-parent = <0x06>;
			interrupts = <0x13>;
			dma-mask = <0x00 0xffffffff>;
			pinctrl-0 = <0x0e>;
			pinctrl-names = "default";
			status = "okay";
			ports-implemented = <0x01>;
		};

		rtc@1fe27800 {
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			compatible = "loongson,ls2k-rtc";
			reg = <0x00 0x1fe27800 0x00 0x100>;
			interrupt-parent = <0x06>;
			interrupts = <0x34>;
		};

		pwm@1fe22000 {
			compatible = "loongson,ls2k-pwm";
			reg = <0x00 0x1fe22000 0x00 0x10>;
			clocks = <0x05 0x0c>;
			clock-names = "pwm-clk";
			interrupt-parent = <0x06>;
			interrupts = <0x18>;
			#pwm-cells = <0x02>;
			pinctrl-0 = <0x0f>;
			pinctrl-names = "default";
			status = "okay";
		};

		pwm@1fe22010 {
			compatible = "loongson,ls2k-pwm";
			reg = <0x00 0x1fe22010 0x00 0x10>;
			clocks = <0x05 0x0c>;
			clock-names = "pwm-clk";
			interrupt-parent = <0x06>;
			interrupts = <0x19>;
			#pwm-cells = <0x02>;
			pinctrl-0 = <0x10>;
			pinctrl-names = "default";
			status = "okay";
		};

		pwm@1fe22020 {
			compatible = "loongson,ls2k-pwm";
			reg = <0x00 0x1fe22020 0x00 0x0f>;
			clocks = <0x05 0x0c>;
			clock-names = "pwm-clk";
			interrupt-parent = <0x06>;
			interrupts = <0x22>;
			#pwm-cells = <0x02>;
			pinctrl-0 = <0x11>;
			pinctrl-names = "default";
			status = "okay";
		};

		pwm@1fe22030 {
			compatible = "loongson,ls2k-pwm";
			reg = <0x00 0x1fe22030 0x00 0x0f>;
			clocks = <0x05 0x0c>;
			clock-names = "pwm-clk";
			interrupt-parent = <0x06>;
			interrupts = <0x23>;
			#pwm-cells = <0x02>;
			pinctrl-0 = <0x12>;
			pinctrl-names = "default";
			status = "okay";
		};

		ethernet@40040000 {
			compatible = "snps,dwmac-3.70a\0ls,ls-gmac";
			reg = <0x00 0x40040000 0x00 0x8000>;
			interrupt-parent = <0x06>;
			interrupts = <0x0c 0x0d>;
			interrupt-names = "macirq\0eth_wake_irq";
			phy-mode = "rgmii";
			bus_id = <0x00>;
			phy_addr = <0xffffffff>;
			dma-mask = <0xffffffff 0xffffffff>;
			status = "okay";
			phy-handle = <0x13>;

			mdio {
				compatible = "snps,dwmac-mdio";
				#address-cells = <0x01>;
				#size-cells = <0x00>;

				ethernet-phy@0 {
					compatible = "ethernet-phy-ieee802.3-c22";
					reg = <0x00>;
					motorcomm,led0_cfg = <0x40>;
					motorcomm,led1_cfg = <0x30>;
					motorcomm,led2_cfg = <0x2600>;
					phandle = <0x13>;
				};
			};
		};

		ethernet@40050000 {
			compatible = "snps,dwmac-3.70a\0ls,ls-gmac";
			reg = <0x00 0x40050000 0x00 0x8000>;
			pinctrl-0 = <0x14>;
			pinctrl-names = "default";
			interrupt-parent = <0x06>;
			interrupts = <0x0e 0x0f>;
			interrupt-names = "macirq\0eth_wake_irq";
			phy-mode = "rgmii";
			bus_id = <0x01>;
			phy_addr = <0xffffffff>;
			dma-mask = <0xffffffff 0xffffffff>;
			status = "okay";
			phy-handle = <0x15>;

			mdio {
				compatible = "snps,dwmac-mdio";
				#address-cells = <0x01>;
				#size-cells = <0x00>;

				ethernet-phy@0 {
					compatible = "ethernet-phy-ieee802.3-c22";
					reg = <0x00>;
					motorcomm,led0_cfg = <0x40>;
					motorcomm,led1_cfg = <0x30>;
					motorcomm,led2_cfg = <0x2600>;
					phandle = <0x15>;
				};
			};
